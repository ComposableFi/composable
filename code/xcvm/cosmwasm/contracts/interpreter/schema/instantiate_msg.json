{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "network_id",
    "registry_address",
    "relayer_address",
    "router_address",
    "user_id"
  ],
  "properties": {
    "network_id": {
      "description": "Network ID of the origin network",
      "allOf": [
        {
          "$ref": "#/definitions/NetworkId"
        }
      ]
    },
    "registry_address": {
      "description": "Address of the XCVM asset registry",
      "type": "string"
    },
    "relayer_address": {
      "description": "Address of the relayer",
      "type": "string"
    },
    "router_address": {
      "description": "Address of the router",
      "type": "string"
    },
    "user_id": {
      "description": "Id of the origin user",
      "type": "array",
      "items": {
        "type": "integer",
        "format": "uint8",
        "minimum": 0.0
      }
    }
  },
  "definitions": {
    "NetworkId": {
      "description": "Newtype for XCVM networks ID. Must be unique for each network and must never change. This ID is an opaque, arbitrary type from the XCVM protocol and no assumption must be made on how it is computed.",
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
