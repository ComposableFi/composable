    uint256 e;uint256 i;
    e += 1 + ProtoBufRuntime._sz_uint64(r.timestamp);
    e += 1 + ProtoBufRuntime._sz_lendelim(r.root.length);
    for(i = 0; i < r.validators.length; i++) {
