{
  "name": "@composable/oracle_setup",
  "version": "1.1.0",
  "description": "Sets up the Oracle with our Price Feed application.",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "start": "yarn oraclesetup",
    "build": "tsc --build",
    "oraclesetup": "npx ts-node -P tsconfig.json src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/*.ts' --fix-dry-run ",
    "clean": "find ./src -name '*.js' -or -name '*.js.map'  -or -name '*.d.ts' -type f -delete"
  },
  "keywords": [
    "oracle",
    "price-feed",
    "composable"
  ],
  "license": "Public Domain",
  "dependencies": {
    "@polkadot/api": "^8.11.3",
    "@polkadot/keyring": "^9.7.2",
    "@polkadot/types": "^8.11.3",
    "@polkadot/types-codec": "^8.11.3",
    "@polkadot/util": "^9.7.2",
    "@polkadot/util-crypto": "^9.7.2",
    "@types/chai": "^4.3.1",
    "@types/node": "^18.0.1",
    "chai": "^4.3.6",
    "eslint": "^8.19.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.8.2",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "tsconfig-paths": "^4.0.0"
  }
}
