{
  "name": "@composable/devnet-setup",
  "version": "1.0.0",
  "description": "Setup devnet",
  "main": "dist/index.js",
  "private": false,
  "scripts": {
    "build": "tsc --build",
    "start": "npx ts-node -P tsconfig.json src/main.ts",
    "lint": "eslint 'src/*.ts' --fix-dry-run ",
    "clean": "find ./src -name '*.js' -or -name '*.js.map'  -or -name '*.d.ts' -type f -delete"
  },
  "keywords": [
    "persistent-devnet",
    "price-feed",
    "composable"
  ],
  "license": "Public Domain",
  "dependencies": {
    "@polkadot/ts": "^0.4.22",
    "@polkadot/api": "^9.5.1",
    "@polkadot/typegen": "^9.5.1",
    "@polkadot/types": "^9.5.1",
    "@polkadot/types-augment": "^9.5.1",
    "@polkadot/types-codec": "^9.5.1",
    "@polkadot/types-create": "^9.5.1",
    "@polkadot/types-known": "^9.5.1",
    "@polkadot/types-support": "^9.5.1",
    "@polkadot/keyring": "^10.1.10",
    "@polkadot/util": "^10.1.10",
    "@polkadot/util-crypto": "^10.2.1",
    "@types/chai": "^4.3.1",
    "@types/node": "^18.0.1",
    "eslint": "^8.19.0",
    "lodash": "^4.17.21",
    "ts-node": "^10.8.2",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "tsconfig-paths": "^4.0.0"
  }
}
