{
  "name": "oraclesetup",
  "version": "1.1.0",
  "description": "Sets up the Oracle with our Price Feed application.",
  "main": "src/index.ts",
  "scripts": {
    "start": "yarn oraclesetup",
    "oraclesetup": "npx ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'src/*.ts' --fix-dry-run ",
    "gen": "yarn run gen:defs && yarn run gen:meta",
    "gen:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package @composable/types/interfaces --input ./src/types/interfaces",
    "gen:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package @composable/types/interfaces --endpoint ws://localhost:9988 --output src/types/interfaces/"
  },
  "keywords": [
    "oracle",
    "price-feed",
    "composable"
  ],
  "author": "@Dom-Roth",
  "license": "Unlicense",
  "dependencies": {
    "@polkadot/api": "^8.6.2",
    "@polkadot/keyring": "^9.3.1",
    "@polkadot/ts": "^0.4.22",
    "@polkadot/typegen": "^8.3.2",
    "@polkadot/types": "^8.3.2",
    "@polkadot/types-augment": "^8.6.2",
    "@polkadot/types-codec": "^8.6.2",
    "@polkadot/types-create": "^8.6.2",
    "@polkadot/types-known": "^8.6.2",
    "@polkadot/types-support": "^8.6.2",
    "@polkadot/util": "^9.3.1",
    "@polkadot/util-crypto": "^9.3.1",
    "@tsconfig/node16": "^1.0.2",
    "@types/chai": "^4.3.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "chai": "^4.3.6",
    "chalk": "^5.0.0",
    "eslint": "^8.15.0",
    "lodash": "^4.17.21",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  }
}
